<OfferPackage Name="Nom fichier offres" PurgeAndReplace="false" xmlns="clr-namespace:Cdiscount.Service.OfferIntegration.Pivot;assembly=Cdiscount.Service.OfferIntegration" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <OfferPackage.Offers>
        <OfferCollection Capacity="1">
            <Offer ProductEan="978-1593274351" SellerProductId="MY_SKU1" ProductCondition="4" Price="10" EcoPart="0.1" Vat="19.36" DeaTax="3.14" Stock="1" PreparationTime="1">
                <Offer.ShippingInformationList>
                    <ShippingInformationList Capacity="2">
                        <ShippingInformation AdditionalShippingCharges="4" DeliveryMode="Standard" ShippingCharges="2" />
                        <ShippingInformation AdditionalShippingCharges="4" DeliveryMode="Tracked" ShippingCharges="2" />
                    </ShippingInformationList>
                </Offer.ShippingInformationList>
            </Offer>
            <Offer ProductEan="978-1593274351" SellerProductId="MY_SKU2" ProductCondition="4" Price="20" EcoPart="0.1" Vat="19.36" DeaTax="3.14" Stock="1" PreparationTime="1">
                <Offer.ShippingInformationList>
                    <ShippingInformationList Capacity="2">
                        <ShippingInformation AdditionalShippingCharges="4" DeliveryMode="Standard" ShippingCharges="2" />
                        <ShippingInformation AdditionalShippingCharges="4" DeliveryMode="Tracked" ShippingCharges="2" />
                    </ShippingInformationList>
                </Offer.ShippingInformationList>
            </Offer>
        </OfferCollection>
    </OfferPackage.Offers>
    <!-- the OfferPublicationList element is optional and is utilized to specify on which website the updates must be done
  by default updates are applied on Cdiscount only (Id=16 for Belgium)
  -->
  <OfferPackage.OfferPublicationList>
      <OfferPublicationList Capacity="3">
          <PublicationPool Id="1" />
          <PublicationPool Id="16" />
          <PublicationPool Id="42" />
      </OfferPublicationList>
  </OfferPackage.OfferPublicationList>
</OfferPackage>
